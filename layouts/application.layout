        <![endif]-->
        <!--<script src="/helpers.js"></script>-->
        <script src="/application.js"></script>
    </head>
    <body>
        <div class='header_container' >
            
                <div class='trim_screen'>
                    <div id='logo_container'>
                        <script id="header_template" type="x-tmpl-mustache/text">
                            {% raw %}<a href='/home'><img class='site_logo' src='{{site_logo}}'/></a>{% endraw %}
                        </script>
                    </div>
                    
                    <div class="desktop_menu">
                        <ul class="desktop_menu_ul">
                            <li>
                                <button onclick='toggle_menu("dir")'>directory</button>
                                <ul class="submenu" id='dir'>
                                    <li>store directory</li>
                                </ul>
                            </li>
                            <li>
                                <button onclick="window.location.href='/map'">centre map</button>
                            </li>
                            <li>
                                <button onclick="window.location.href='/promotions'">promotions </button>
                            </li>
                            <li>
                                <button onclick='toggle_menu("loc")'>location</button>
                                <ul class="submenu" id='loc'>
                                    <li>Gift Cards</li>
                                    <li>services</li>
                                    <li>shopping hours</li>
                                    <li>contact us</li>
                                </ul>
                            </li>
                            <li>
                                <button onclick='toggle_menu("about")'>about us</button>
                                <ul class="submenu" id='about'>
                                    <li>Leasing</li>
                                    <li>Contact Us</li>
                                </ul>
                            </li>
                         
                        </ul>
                    </div>
                    
                </div>
        </div>
        <div class='page_content'>
            {{yield}} 
            <div class="right_block" id="right_block_container">
                <script id="right_block_template" type="x-tmpl-mustache/text">
                    {% raw %}
                        <img src='{{image_url}}'/>
                    {% endraw %}
                </script>
            </div>
        </div>
        <div class='bottom_nav'>
            
        </div>    
        <div class='footer_container'>
            
        </div>
    </body>
</html>

<script>
   $(document).ready(function() {
        function renderAll(){
            var propertyDetails = getPropertyDetails();
            renderPageData("#logo_container", "#header_template", propertyDetails, "property")
            
            var featureList = getFeatureList().reverse();
            renderPageData("#right_block_container", "#right_block_template", featureList, "right_block")
        }
        loadMallDataCached(renderAll);  
   })
</script>
