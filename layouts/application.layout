<html lang="en">
    <head>
        <meta name="viewport" content="width=device-width, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no,initial-scale=1">
        <title>{{meta_title}}</title>
        
        <!-- css -->
        <link type="text/css" rel="stylesheet" href="https://testbucketblurt.s3-us-west-2.amazonaws.com/sites/540373d34a616d0cccaf0000/368584aaeda8ec2d1f80a5f777235d72/font-awesome.min.css">
        <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css" rel="stylesheet">
        <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
        <link href="https://cdnjs.cloudflare.com/ajax/libs/flexslider/2.5.0/flexslider.css" rel="stylesheet">
        <link rel="stylesheet" href="/application.css">
        <link href='http://fonts.googleapis.com/css?family=Maven+Pro:400,500,700,900' rel='stylesheet' type='text/css'>
        <!-- js -->
        <script src="//code.jquery.com/jquery-1.11.0.min.js"></script>
        <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-ajaxtransport-xdomainrequest/1.0.2/jquery.xdomainrequest.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/mustache.js/0.8.1/mustache.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.8.3/moment.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
        <!--MallMaverick v1.0-->
        <script src="http://mallmaverick.kodekloud.io/mallmaverick.js?v=0.3"></script>
        <script src="http://mallmaverick.kodekloud.io/mustache_mallmaverick.js?v=0.3"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/flexslider/2.5.0/jquery.flexslider.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.4.1/jquery.cookie.min.js"></script>
        
        <script>
            setEndPoint('http://htc.mallmaverick.com/api/v2/htc/all.json');
            sessionStorage.setItem('primary_locale', 'en-CA');
            sessionStorage.setItem('secondary_locale', 'fr-CA');
        </script>
        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
            <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->
        <!--<script src="/helpers.js"></script>-->
        <script src="/application.js"></script>
    </head>
    <body>
        <div class='header_container' >
            
                <div class='trim_screen'>
                    <div id='logo_container'>
                        <script id="header_template" type="x-tmpl-mustache/text">
                            {% raw %}<a href='/home'><img class='site_logo' src='{{site_logo}}'/></a>{% endraw %}
                        </script>
                    </div>
                    
                    <div class="desktop_menu">
                        <ul class="desktop_menu_ul">
                            <li>
                                <button onclick='toggle_menu("dir")'>directory</button>
                                <ul class="submenu" id='dir'>
                                    <li>store directory</li>
                                </ul>
                            </li>
                            <li>
                                <button onclick="window.location.href='/map'">centre map</button>
                            </li>
                            <li>
                                <button onclick="window.location.href='/promotions'">promotions </button>
                            </li>
                            <li>
                                <button onclick='toggle_menu("loc")'>location</button>
                                <ul class="submenu" id='loc'>
                                    <li>Gift Cards</li>
                                    <li>services</li>
                                    <li>shopping hours</li>
                                    <li>contact us</li>
                                </ul>
                            </li>
                            <li>
                                <button onclick='toggle_menu("about")'>about us</button>
                                <ul class="submenu" id='about'>
                                    <li>Leasing</li>
                                    <li>Contact Us</li>
                                </ul>
                            </li>
                         
                        </ul>
                    </div>
                    
                </div>
        </div>
        <div class='page_content'>
            {{yield}} 
            <div class="right_block" id="right_block_container">
                <script id="right_block_template" type="x-tmpl-mustache/text">
                    {% raw %}
                        <img src='{{image_url}}'/>
                    {% endraw %}
                </script>
            </div>
        </div>
        <div class='bottom_nav'>
            
        </div>    
        <div class='footer_container'>
            
        </div>
    </body>
</html>

<script>
   $(document).ready(function() {
        function renderAll(){
            var propertyDetails = getPropertyDetails();
            renderPageData("#logo_container", "#header_template", propertyDetails, "property")
        }
        loadMallDataCached(renderAll);  
   })
</script>
